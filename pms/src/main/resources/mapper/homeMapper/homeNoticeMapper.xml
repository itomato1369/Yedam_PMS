<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pms.home.notice.mapper.HomeNoticeMapper">
	<select id="findNotices" resultType="HomeNoticeDto">
		SELECT
		notice_no,
		title,
		add_date
		FROM notice
		ORDER BY add_date DESC, notice_no DESC
	</select>
</mapper>